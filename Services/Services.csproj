<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>BootCom-Maui-Shared-Services</PackageId>
    <Title>BootCom Services For Maui</Title>
    <Version>1.0.0</Version>
    <Authors>Chris Boot</Authors>
    <Product>BootCom-Maui-Shared-Services</Product>
    <Description>Services used for the BootCom .NET Maui shared libraries</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BootCom-Maui-Shared-Extensions" Version="1.0.0" />
    <PackageReference Include="BootCom-Maui-Shared-Interfaces" Version="1.0.2" />
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="6.0.0" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="7.0.3" />
  </ItemGroup>

</Project>
